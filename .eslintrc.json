{
  "extends": ["plugin:@next/next/recommended", "./node_modules/gts/"],
  "settings": {
    "next": {
      "rootDir": "renderer/"
    }
  },
  "rules": {
    "node/no-unpublished-import": [
      "error",
      {
        "allowModules": ["electron", "next"]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
